datasource db {
  provider = "sqlite"
  url      = "file:data.db"
}

generator client {
  provider = "prisma-client-js"
}

model Story {
  id     Int      @id @default(autoincrement())
  name   String
  author String
  date   DateTime
}

model Feature {
  id          Int    @id @default(autoincrement())
  name        String
  description String
}
